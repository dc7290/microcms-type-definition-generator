{
  "name": "microcms-type-definition-generator",
  "license": "UNLICENSED",
  "scripts": {
    "analyze": "ANALYZE=true yarn build",
    "create:fc": "hygen new fc",
    "create:pc": "hygen new pc",
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "pathpida --watch --output src/utils",
    "build": "pathpida --output src/utils && next build",
    "start": "next start",
    "fix": "prettier --write './**/*.{js,jsx,ts,tsx}'",
    "lint": "next lint . --cache",
    "lint:fix": "next lint . --cache --fix",
    "lint-staged": "lint-staged",
    "tsc": "tsc",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@headlessui/react": "1.4.2",
    "@hookform/error-message": "2.0.0",
    "clsx": "1.1.1",
    "dayjs": "1.10.7",
    "focus-visible": "5.2.0",
    "framer-motion": "5.5.5",
    "next": "12.0.7",
    "next-head-seo": "0.1.3",
    "react": "17.0.2",
    "react-aria": "3.12.0",
    "react-dom": "17.0.2",
    "react-hook-form": "7.22.5",
    "react-spinners": "0.11.0",
    "react-use": "17.3.2",
    "tailwindcss": "3.0.10"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "12.0.7",
    "@types/node": "16.11.17",
    "@types/react": "17.0.38",
    "@typescript-eslint/eslint-plugin": "5.9.0",
    "@typescript-eslint/parser": "5.9.0",
    "autoprefixer": "10.4.1",
    "eslint": "7.23.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "8.3.0",
    "husky": "7.0.4",
    "hygen": "6.1.0",
    "lint-staged": "12.1.5",
    "npm-run-all": "4.1.5",
    "pathpida": "0.17.0",
    "postcss": "8.4.5",
    "postcss-import": "14.0.2",
    "prettier": "2.5.1",
    "typescript": "4.5.4"
  }
}
